<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="AdyenPayment\AdyenApi\TransportFactory">
            <argument type="service" id="AdyenPayment\AdyenApi\HttpClient\ClientFactory"/>
        </service>
        <service id="AdyenPayment\AdyenApi\HttpClient\ClientFactory">
            <argument type="service" id="AdyenPayment\Components\Configuration"/>
            <argument type="service" id="adyen_payment.logger"/>
        </service>
        <service id="AdyenPayment\AdyenApi\HttpClient\ClientMemoise">
            <argument type="service" id="AdyenPayment\AdyenApi\HttpClient\ClientFactory"/>
        </service>
        <service id="AdyenPayment\AdyenApi\Recurring\DisableTokenRequestHandler" public="true">
            <argument type="service" id="AdyenPayment\AdyenApi\TransportFactory"/>
            <argument type="service" id="AdyenPayment\Session\CustomerNumberProvider"/>
        </service>
    </services>
</container>