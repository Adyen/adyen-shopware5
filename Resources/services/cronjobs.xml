<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Cronjobs -->
        <service id="meteor_adyen.subscriber.cronjob.process_notifications" class="MeteorAdyen\Subscriber\Cronjob\ProcessNotifications">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.components.fifo_notification_loader"/>
            <argument type="service" id="meteor_adyen.components.notification_processor"/>
        </service>
    </services>
</container>
