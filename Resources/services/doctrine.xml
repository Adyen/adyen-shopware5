<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="AdyenPayment\Doctrine\Writer\PaymentMethodWriter">
            <argument type="service" id="models"/>
            <argument type="service" id="AdyenPayment\Models\Payment\PaymentFactory"/>
            <argument type="service" id="AdyenPayment\Doctrine\Writer\PaymentAttributeWriter"/>
            <argument type="service" id="AdyenPayment\Shopware\Repository\PaymentRepository"/>
        </service>
        <service id="AdyenPayment\Doctrine\Writer\PaymentAttributeWriter">
            <argument type="service" id="shopware_attribute.data_persister"/>
            <argument type="service" id="AdyenPayment\Shopware\Crud\AttributeWriter"/>
        </service>
        <service id="AdyenPayment\Doctrine\Writer\TraceableLegacyPaymentMethodWriterDecorator"
                 decorates="AdyenPayment\Doctrine\Writer\PaymentMethodWriter">
            <argument type="service" id="AdyenPayment\Doctrine\Writer\TraceableLegacyPaymentMethodWriterDecorator.inner"/>
            <argument type="service" id="AdyenPayment\Shopware\Repository\PaymentRepository"/>
            <argument type="service" id="AdyenPayment\Doctrine\Writer\PaymentAttributeWriter" />
            <argument type="service" id="adyen_payment.logger" />
        </service>
    </services>
</container>
