<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Console commands -->
        <service id="AdyenPayment\Commands\ProcessNotifications">
            <argument type="service" id="AdyenPayment\Components\FifoNotificationLoader"/>
            <argument type="service" id="AdyenPayment\Components\NotificationProcessor"/>
            <tag name="console.command" command="adyen:payment:process:notifications" />
        </service>

        <service id="AdyenPayment\Commands\ImportPaymentMethodsCommand">
            <argument type="service" id="AdyenPayment\Import\PaymentMethodImporter" />
            <tag name="console.command" command="adyen:import:payment-methods" />
        </service>
    </services>
</container>
