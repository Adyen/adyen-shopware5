<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="AdyenPayment\Shopware\Repository\PaymentRepository">
            <argument type="service" id="models"/>
        </service>

        <service id="AdyenPayment\Models\RecurringPayment\RecurringPaymentTokenRepository">
            <argument type="service" id="models"/>
        </service>
        <service id="AdyenPayment\Models\RecurringPayment\TraceableRecurringPaymentTokenRepository"
                 decorates="AdyenPayment\Models\RecurringPayment\RecurringPaymentTokenRepository">
            <argument type="service" id="AdyenPayment\Models\RecurringPayment\TraceableRecurringPaymentTokenRepository.inner"/>

            <argument type="service" id="adyen_payment.logger" />
        </service>

        <service id="AdyenPayment\Shopware\Crud\AttributeWriter">
            <argument type="service" id="shopware_attribute.crud_service"/>
            <argument type="service" id="models"/>
        </service>
    </services>
</container>



