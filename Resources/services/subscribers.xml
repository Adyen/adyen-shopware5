<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Backend Subscribers -->
        <service id="AdyenPayment\Subscriber\Backend\ExtendViewTemplateSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Backend\BackendJavascriptSubscriber">
            <argument>%adyen_payment.plugin_dir%</argument>
            <argument type="expression">service('models').getRepository('AdyenPayment\\Models\\Notification')</argument>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Backend\BackendOrderSubscriber">
            <argument type="expression">service('models').getRepository('AdyenPayment\\Models\\PaymentInfo')</argument>
            <argument type="service" id="AdyenPayment\Components\NotificationManager"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Backend\ImportSubShopPaymentMethodsSubscriber">
            <argument type="expression">service('models').getRepository('Shopware\\Models\\Shop\\Shop')</argument>
            <argument type="service" id="AdyenPayment\Rule\AdyenApi\IsMainShopConfigRule"/>
            <argument type="service" id="AdyenPayment\Dbal\Writer\Payment\PaymentMeansSubShopsWriter"/>
            <argument type="service" id="AdyenPayment\Import\PaymentMethodImporter" />
            <tag name="shopware.event_subscriber"/>
            <tag name="kernel.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Backend\RemoveSubShopPaymentMethodSubscriber">
            <argument type="service" id="AdyenPayment\Dbal\Remover\PaymentMeanSubShopRemover" />
            <tag name="shopware.event_subscriber"/>
            <tag name="kernel.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Applepay\MerchantAssociation\PerformanceLoaderSubscriber">
            <argument>%adyen_payment.plugin_dir%</argument>
        </service>
        <service id="AdyenPayment\Subscriber\Backend\HideStoredPaymentsSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>

        <!-- Frontend Subscribers -->
        <service id="AdyenPayment\Subscriber\AddPluginTemplatesSubscriber">
            <argument>%adyen_payment.plugin_dir%</argument>
            <argument type="service" id="template"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\EnrichPaymentSubscriber">
            <argument type="service" id="AdyenPayment\Components\Adyen\PaymentMethod\EnrichedPaymentMeanProvider" />
            <argument type="service" id="AdyenPayment\Shopware\Serializer\SwPaymentMeanCollectionSerializer" />
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\OrderEmailSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="models"/>
            <argument type="service" id="AdyenPayment\Components\OrderMailService"/>
        </service>
        <service id="AdyenPayment\Subscriber\OrderStoredMethodIdSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="models"/>
        </service>
        <service id="AdyenPayment\Subscriber\CheckoutSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="AdyenPayment\Components\Configuration"/>
            <argument type="service" id="AdyenPayment\Components\Adyen\PaymentMethodService"/>
            <argument type="service" id="AdyenPayment\Components\DataConversion" />
            <argument type="service" id="AdyenPayment\Components\Adyen\PaymentMethod\EnrichedPaymentMeanProvider"/>
            <argument type="service" id="AdyenPayment\Components\Adyen\Builder\PaymentMethodOptionsBuilder"/>
            <argument type="service" id="AdyenPayment\Shopware\Serializer\SwPaymentMeanCollectionSerializer"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\AddAdyenSourceTypeToViewSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\AddGooglePayConfigToViewSubscriber">
            <argument type="service" id="AdyenPayment\Components\WebComponents\GooglePayConfigProvider"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\RegisterConfirmSnippetsSubscriber">
            <argument type="service" id="snippets"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\RegisterPaymentSnippetsSubscriber">
            <argument type="service" id="snippets"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Notification\LogIncomingNotificationSubscriber">
            <argument type="service" id="adyen_payment.logger.notifications"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Notification\UpdateOrderPsPSubscriber">
            <argument type="service" id="AdyenPayment\Components\Manager\OrderManager"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\AddErrorMessageToViewSubscriber">
            <argument type="service" id="AdyenPayment\Session\ErrorMessageProvider"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\EnrichUserAdditionalPaymentSubscriber">
            <argument type="service" id="AdyenPayment\Components\Adyen\PaymentMethod\EnrichedPaymentMeanProvider"/>
            <argument type="service" id="session"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\EnrichUmbrellaPaymentMeanSubscriber">
            <argument type="service" id="session"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Checkout\PersistStoredMethodIdSubscriber">
            <argument type="service" id="session"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Applepay\MerchantAssociation\RegisterUrlCountSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
    </services>
</container>
