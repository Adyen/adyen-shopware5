<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Subscribers -->
        <service id="meteor_adyen.subscriber.template" class="MeteorAdyen\Subscriber\Template">
            <argument>%meteor_adyen.plugin_dir%</argument>
            <argument type="service" id="template"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="meteor_adyen.subscriber.account_payment_subscriber" class="MeteorAdyen\Subscriber\AccountPaymentSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.components.payment_method_service"/>
            <argument type="service" id="session"/>
        </service>
        <service id="meteor_adyen.subscriber.backend_config_subscriber" class="MeteorAdyen\Subscriber\BackendConfigSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.components.origin_keys_service"/>
            <argument type="service" id="meteor_adyen.logger"/>
            <argument type="service" id="meteor_adyen.components.shopware_version_check"/>
        </service>
        <service id="meteor_adyen.subscriber.backend_javascript_subscriber" class="MeteorAdyen\Subscriber\BackendJavascriptSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument>%meteor_adyen.plugin_dir%</argument>
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.subscriber.backend_payment_subscriber" class="MeteorAdyen\Subscriber\BackendPaymentSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="meteor_adyen.subscriber.payment" class="MeteorAdyen\Subscriber\PaymentSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.components.adyen.payment.method"/>
            <argument type="service" id="meteor_adyen.components.payment_method_service"/>
        </service>
        <service id="meteor_adyen.subscriber.backend_order_subscriber" class="MeteorAdyen\Subscriber\BackendOrderSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="models"/>
            <argument type="service" id="meteor_adyen.components.notification_manager"/>
        </service>
        <service id="meteor_adyen.subscriber.checkout" class="MeteorAdyen\Subscriber\CheckoutSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.components.configuration"/>
            <argument type="service" id="meteor_adyen.components.adyen.payment.method"/>
            <argument type="service" id="meteor_adyen.components.payment_method_service"/>
            <argument type="service" id="session"/>
            <argument type="service" id="models"/>
            <argument type="service" id="snippets"/>
            <argument type="service" id="front" />
            <argument type="service" id="meteor_adyen.components.manager.adyen_manager" />
            <argument type="service" id="meteor_adyen.components.data_conversion" />
        </service>
        <service id="meteor_adyen.subscriber.frontend_payment_name_subscriber" class="MeteorAdyen\Subscriber\FrontendPaymentNameSubscriber">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.components.payment_method_service"/>
            <argument type="service" id="meteor_adyen.logger"/>
        </service>
        <service id="meteor_adyen.subscriber.notification.log_incoming_notification" class="MeteorAdyen\Subscriber\Notification\LogIncomingNotification">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
        </service>
    </services>
</container>
