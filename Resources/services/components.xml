<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Components -->
        <service id="meteor_adyen.components.data_conversion" class="MeteorAdyen\Components\DataConversion"/>
        <service id="meteor_adyen.components.notification_processor" class="MeteorAdyen\Components\NotificationProcessor">
            <argument type="tagged" tag="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
        </service>
        <service id="meteor_adyen.components.configuration" class="MeteorAdyen\Components\Configuration">
            <argument type="service" id="shopware.plugin.cached_config_reader"/>
            <argument type="service" id="dbal_connection"/>
        </service>
        <service id="meteor_adyen.components.adyen.apifactory" class="MeteorAdyen\Components\Adyen\ApiFactory">
            <argument type="service" id="meteor_adyen.components.configuration"/>
            <argument type="service" id="meteor_adyen.logger"/>
        </service>
        <service id="meteor_adyen.components.adyen.payment.method"
                 class="MeteorAdyen\Components\Adyen\PaymentMethodService">
            <argument type="service" id="meteor_adyen.components.adyen.apifactory"/>
            <argument type="service" id="meteor_adyen.components.configuration"/>
        </service>
        <service id="meteor_adyen.components.adyen.refund_service"
                 class="MeteorAdyen\Components\Adyen\RefundService">
            <argument type="service" id="meteor_adyen.components.adyen.apifactory"/>
            <argument type="service" id="models"/>
            <argument type="service" id="meteor_adyen.components.notification_manager"/>
        </service>
        <service id="meteor_adyen.components.notification_manager"
                 class="MeteorAdyen\Components\NotificationManager">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payment_status_update" class="MeteorAdyen\Components\PaymentStatusUpdate">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payment_method_service" class="MeteorAdyen\Components\PaymentMethodService">
            <argument type="service" id="models"/>
            <argument type="service" id="session"/>
            <argument type="service" id="snippets"/>
            <argument type="service" id="meteor_adyen.components.adyen.payment.method"/>

        </service>
        <service id="meteor_adyen.components.builder.notification_builder" class="MeteorAdyen\Components\Builder\NotificationBuilder">
            <argument type="service" id="meteor_adyen.components.data_conversion"/>
            <argument type="service" id="models"/>
        </service>

        <!-- Payload -->
        <service id="meteor_adyen.components.payload.chain" class="MeteorAdyen\Components\Payload\Chain">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payload.payment_payload_provider" class="MeteorAdyen\Components\Payload\PaymentPayloadProvider">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payload.providers.application_info_provider" class="MeteorAdyen\Components\Payload\Providers\ApplicationInfoProvider">
            <argument type="service" id="models"/>
            <argument type="service" id="meteor_adyen.components.configuration"/>
        </service>
        <service id="meteor_adyen.components.payload.providers.browser_info_provider" class="MeteorAdyen\Components\Payload\Providers\BrowserInfoProvider">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payload.providers.lineitems_info_Provider" class="MeteorAdyen\Components\Payload\Providers\LineItemsInfoProvider">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payload.providers.order_info_provider" class="MeteorAdyen\Components\Payload\Providers\OrderInfoProvider">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payload.providers.paymentmethod_provider" class="MeteorAdyen\Components\Payload\Providers\PaymentMethodProvider">
            <argument type="service" id="models"/>
        </service>
        <service id="meteor_adyen.components.payload.providers.shopper_info_provider" class="MeteorAdyen\Components\Payload\Providers\ShopperInfoProvider">
            <argument type="service" id="models"/>
        </service>


        <!-- Notification Processors -->
        <service id="meteor_adyen.components.notification_processor.authorisation"
                 class="MeteorAdyen\Components\NotificationProcessor\Authorisation">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.cancellation"
                 class="MeteorAdyen\Components\NotificationProcessor\Cancellation">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.capture"
                 class="MeteorAdyen\Components\NotificationProcessor\Capture">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.capture_failed"
                 class="MeteorAdyen\Components\NotificationProcessor\CaptureFailed">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.chargeback"
                 class="MeteorAdyen\Components\NotificationProcessor\Chargeback">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.chargeback_reversed"
                 class="MeteorAdyen\Components\NotificationProcessor\ChargebackReversed">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.refund"
                 class="MeteorAdyen\Components\NotificationProcessor\Refund">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.refund_failed"
                 class="MeteorAdyen\Components\NotificationProcessor\RefundFailed">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
        <service id="meteor_adyen.components.notification_processor.refunded_reversed"
                 class="MeteorAdyen\Components\NotificationProcessor\RefundedReversed">
            <tag name="meteor.adyen.notificationprocessor"/>
            <argument type="service" id="meteor_adyen.logger.notifications"/>
            <argument type="service" id="events"/>
            <argument type="service" id="meteor_adyen.components.payment_status_update"/>
        </service>
    </services>
</container>
