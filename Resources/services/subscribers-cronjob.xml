<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Cronjob Subscribers -->
        <service id="AdyenPayment\Subscriber\Cronjob\ProcessNotifications">
            <tag name="shopware.event_subscriber"/>
            <argument type="service" id="AdyenPayment\Components\FifoNotificationLoader"/>
            <argument type="service" id="AdyenPayment\Components\FifoTextNotificationLoader"/>
            <argument type="service" id="AdyenPayment\Components\NotificationProcessor"/>
            <argument type="service" id="AdyenPayment\Components\IncomingNotificationManager"/>
            <argument type="service" id="adyen_payment.logger.notifications" />
        </service>
        <service id="AdyenPayment\Subscriber\Cronjob\ImportPaymentMethodSubscriber">
            <argument type="service" id="AdyenPayment\Import\PaymentMethodImporter"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="AdyenPayment\Subscriber\Applepay\MerchantAssociation\AddSeoUrlSubscriber">
            <argument type="service" id="AdyenPayment\Applepay\MerchantAssociation\RewriteUrl\SeoUrlWriter"/>
            <tag name="shopware.event_subscriber"/>
        </service>
    </services>
</container>
