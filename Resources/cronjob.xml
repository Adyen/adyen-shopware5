<?xml version="1.0" encoding="utf-8"?>
<cronjobs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/shopware/shopware/5.3/engine/Shopware/Components/Plugin/schema/cronjob.xsd">
    <cronjob>
        <name>Adyen: Process Notifications</name>
        <action>Shopware_CronJob_AdyenPaymentProcessNotifications</action>
        <active>true</active>
        <interval>60</interval>
        <disableOnError>false</disableOnError>
    </cronjob>
    <cronjob>
        <name>Adyen: Import Payment Methods</name>
        <action>AdyenPayment_CronJob_ImportPaymentMethods</action>
        <active>true</active>
        <interval>86400</interval>
        <disableOnError>false</disableOnError>
    </cronjob>
</cronjobs>